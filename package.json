{
	"name": "arpirobot",
	"displayName": "ArPiRobot",
	"description": "VSCode Integration for ArPiRobot development",
	"version": "0.1.11",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"files": [
		"templates/"
	],
	"contributes": {
		"commands": [
			{
				"command": "arpirobot.createProject",
				"title": "ArPiRobot - Create ArPiRobot Project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"fs-extra": "^10.0.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"publisher": "marcusbehel"
}
