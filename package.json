{
	"name": "arpirobot",
	"displayName": "ArPiRobot",
	"description": "VSCode Integration for ArPiRobot development",
	"version": "0.0.11",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "arpirobot.createProject",
				"title": "ArPiRobot - Create PythonLib Project"
			},
			{
				"command": "arpirobot.updateDevEnv",
				"title": "ArPiRobot - Install Update (for currently selected python version)"
			},
			{
				"command": "arpirobot.createJavaProject",
				"title": "ArPiRobot - Create JavaLibProject"
			},
			{
				"command": "arpirobot.gradlebuild",
				"title": "ArPiRobot - Build JavaLib Project"
			},
			{
				"command": "arpirobot.gradleclean",
				"title": "ArPiRobot - Clean JavaLib Project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"adm-zip": "0.4.14",
		"tmp": "0.1.0",
		"child-process": "1.0.2"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"@types/adm-zip": "0.4.31",
		"@types/tmp": "0.0.33",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"publisher": "marcusbehel"
}
